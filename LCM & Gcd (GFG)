import java.util.*;

public class LCMGCD {
    
    // Function to compute GCD using Euclidean Algorithm
    static int gcd(int a, int b) {
        while (b != 0) {
            int temp = b;
            b = a % b;
            a = temp;
        }
        return a;
    }
    
    // Function to compute LCM
    static int lcm(int a, int b, int gcdValue) {
        return (a * b) / gcdValue;
    }
    
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int a = sc.nextInt();
        int b = sc.nextInt();
        
        int gcdValue = gcd(a, b);
        int lcmValue = lcm(a, b, gcdValue);
        
        // Print result as array format
        System.out.println("[" + lcmValue + ", " + gcdValue + "]");
        
        sc.close();
    }
}
